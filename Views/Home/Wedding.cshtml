<h1>Plan Your Wedding</h1>
<hr>

<form asp-action="NewWedding"  asp-controller="Home" method="post">
    <span asp-validation-for="WedderOne"></span><br>
    <label>Wedder One</label>
    <input type="text" asp-for="WedderOne"><br>

    <span asp-validation-for="WedderTwo"></span><br>
    <label>Wedder Two</label>
    <input type="text" asp-for="WedderTwo"><br>

    <span asp-validation-for="Date"></span><br>
    <label>Date</label>
    <input type="date" asp-for="Date"><br>

    <span asp-validation-for="Address"></span><br>
    <label>Wedding Address</label>
    <input type="text" asp-for="Address"><br>

    <input type="submit" value="Create">
</form>